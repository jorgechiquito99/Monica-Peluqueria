window.onload = () => {
  var elem = document.getElementsByClassName("no-top");
  if (!!elem.length) {
    // If there is no element called no-top, exit
    return;
  }
  console.log("adding padding");
  // get the header
  var header = document.getElementsByClassName("header-wrapper")[0];
  // get header size
  var size = header.clientHeight;
  // get body
  var body = header.nextElementSibling;
  body.style = "margin-top:95px;";

};
